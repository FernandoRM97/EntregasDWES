<?xml version="1.0" encoding="UTF-8"?>
<web-app
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
    version="4.0">

	<display-name>Archetype Created Web Application</display-name>
	
	<servlet>
  		<display-name>mostrarEmpleadosController</display-name>
    	<servlet-name>mostrarEmpleadosController</servlet-name>
  		<servlet-class>Servlets.mostrarEmpleadosController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>mostrarEmpleadosController</servlet-name>
		<url-pattern>/mostrarEmpleadosController</url-pattern>
	</servlet-mapping>
	
	<servlet>
  		<display-name>mostrarSalarioEmpleado</display-name>
    	<servlet-name>mostrarSalarioEmpleado</servlet-name>
  		<servlet-class>Servlets.mostrarSalarioEmpleado</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>mostrarSalarioEmpleado</servlet-name>
		<url-pattern>/mostrarSalarioEmpleado</url-pattern>
	</servlet-mapping>
  	
  		<servlet>
  		<display-name>modificarEmpleadoController</display-name>
    	<servlet-name>modificarEmpleadoController</servlet-name>
  		<servlet-class>Servlets.modificarEmpleadoController</servlet-class>
	</servlet>
	
  	<servlet>
  		<display-name>controladorUnico</display-name>
  		<servlet-name>controladorUnico</servlet-name>
  		<servlet-class>Servlets.controladorUnico</servlet-class>
  	</servlet>
  		
  		<servlet-mapping>
		<servlet-name>controladorUnico</servlet-name>
		<url-pattern>/controladorUnico</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>modificarEmpleadoController</servlet-name>
		<url-pattern>/modificarEmpleadoController</url-pattern>
	</servlet-mapping>
  	
  	<welcome-file-list>
  		<welcome-file>Menu.html</welcome-file>
  	</welcome-file-list>

  
</web-app>
